Global =  {\key g\major \time 3/4}

RH = \relative g' {
\partial 4*1 d4
<d' b>8[ d, <d' b>8 d, <c' a> d,] |
b'[ g c a d b] |
<< {\stemDown c4 c c} \\ {\stemUp e4 e e} >> |
<< {\stemDown c2 b4} \\ {\stemUp e2 d4} >> |
r8 c[ a' c, a' c,] |
r b[ g' b, g' b,] |
r a[ a( g a b)] |
g2 r4
}

LH = \relative a {
\clef bass
\partial 4*1 r4
g,4 b d | g2 r4 |
e8[ d c d e fis] |
g[ fis g fis] g4 |
fis d' d | g, d' d |
d, << {c' c} \\ {a a} >> |
g g, r |
}

